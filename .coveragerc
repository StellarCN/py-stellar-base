[run]
source =
    stellar_sdk/
    tests/

[report]
omit =
    stellar_sdk/xdr/*
    stellar_sdk/vendor/*
    setup.py

exclude_lines =
    pragma: no cover

    def __repr__
    def __str__
    if self\.debug

    raise AssertionError
    raise NotImplementedError

    pass

    if 0:
    if __name__ == .__main__.:
